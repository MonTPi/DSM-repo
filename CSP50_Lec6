##Debugging
from ast import Break


def main():
    while True:
        try:
            hight = int(input("hight: "))
            pyramid(hight) 
        except ValueError:
            print("must be number!")
        break
    

def pyramid(n):
    for i in range(n):
        print("#" * (i+1))
    
    

if __name__== "__main__":
    main()

