def main():
  column(int(input('how high? ')))
  row(int(input('how long? ')))
  block(int(input('how big? ')))

def column(hight):
    for _ in range(hight):
     print('#')

def row(length):
    print('|?|' * length )

def block(size):
    for _ in range(size):
     print("#" * size, sep=' ')

main()

